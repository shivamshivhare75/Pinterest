<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-800 text-white px-10 py-8">
  <div class="cards flex flex-wrap gap-10">
    <% const uniqueLikedPosts = [];%>
    <%user.liked.forEach((ele) => {%>
      <%const exists = uniqueLikedPosts.some((post) =>  post._id === ele._id);%>
     <% if (!exists) { %>
       <% uniqueLikedPosts.push(ele); %>
        <div class="card w-40">
          <div class="w-full h-40 bg-zinc-700 rounded-lg overflow-hidden">
            <img src="/images/uploads/<%= ele.image %>" class="w-full h-full object-cover" alt="" />
          </div>
          <h5 class="mt-2 text-sm"><%= ele.title %></h5>
          <h6 class="text-xs font-semibold opacity-60 tracking-wide">@<%= ele.user.username %></h6>
        </div>
    <% } %>
  <%}); %>
  </div>
</div>
<% include ./partials/footer.ejs %>
