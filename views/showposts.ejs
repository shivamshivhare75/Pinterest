<% include ./partials/header.ejs%>
<div class="w-full min-h-screen bg-zinc-800 text-white px-10 py-8">
  <div class="cards flex flex-wrap gap-10">
    <% user.posts.forEach((ele)=>{%>
    <div class="card w-40">
      <div class="w-full h-40 bg-sky-200 rounded-lg overflow-hidden">
        <img
          src="/images/uploads/<%= ele.image%>"
          class="w-full h-full object-cover"
          alt=""
        />
      </div>
      <h5 class="mt-2 text-sm"><%= ele.title%></h5>
      <h6 class="mt-2 text-sm"><%= ele.description%></h6>
    </div>
    <%})%>
  </div>
</div>
<% include ./partials/footer.ejs%>
